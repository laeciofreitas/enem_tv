<% form_for @exam do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :tag_names, "Tags"%> <br/>
    <%= f.text_field :tag_names %>
  </p>
  <% f.fields_for :questions do |builder|%>
    <%= render "question_fields", :f => builder%>
  <% end %>
  <p><%= f.submit "Submit" %></p>
<% end %>

